Simulation sim {
  .steps = 160
  Stage fecundity {
    .initial = 1
    .growthFactor = 400
    .duration = 55
    .k = 4
  }
  OutputR {
    PageR {
      PlotR {
        .ports = fecundity[outflow]
        .ggplot = "geom_line(size=1.1) + 
                    scale_x_continuous(breaks=seq(0,140,20)) +
                    scale_y_continuous(breaks=seq(0,8,1))"
      }
    }
  }
}

