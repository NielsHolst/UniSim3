// aphid-juvenile-survival.box
Simulation sim {
  .steps = temperature[steps]
  SequenceByStep temperature {
    .min = 15
    .max = 35
    .by = 0.5
  }
  AphidJuvenileSurvival survival {
    .cropGrowthStage = 60
    .temperature = temperature[value]
  }
  OutputR {
    PageR {
      .xAxis = temperature[value]
      PlotR {
        .ports = survival[value]
      }
    }
  }
}